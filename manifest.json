{
  "manifest_version": 2,

  "name": "用谷歌百度一下！",
  "description": "修改百度为谷歌主题，可以假装自己在使用谷歌搜索！",
  "version": "1.0",
  "homepage_url":"https://github.com/xiongwilee/baidu2google",
  "icons": 
  { 
    "16": "static/image/icon.png",
    "48": "static/image/icon.png",
    "128": "static/image/icon.png"
  },
  "content_scripts": [
    {
      "matches": ["https://www.baidu.com/*"],
      "js": ["static/js/main.js"]
    }
  ],
  "permissions": [],
  "web_accessible_resources": [
    "static/css/main_index_no_login.css",
    "static/css/main_result.css",
    "static/image/new_baidu_favicon.ico"
  ]
}
