{
  "manifest_version": 2,

  "name": "用谷歌百度一下！",
  "description": "修改百度为谷歌主题，可以假装自己在使用谷歌搜索！",
  "version": "1.0",
  "homepage_url":"https://github.com/xiongwilee/baidu2google",
  "icons": 
  { 
    "16": "static/icon.png",             
    "48": "static/icon.png",            
    "128": "static/icon.png" 
  },
  "content_scripts": [
    {
      "matches": ["https://www.baidu.com/*"],
      "js": ["static/main.js"]
    }
  ],
  "permissions": [],
  "web_accessible_resources": [
    "static/main.css",
    "static/new_baidu_favicon.ico"
  ]
}
